<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants">

    <TextView
        android:id="@+id/list_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="49dp"
        android:layout_weight="0.03"
        android:text="nombre"
        tools:ignore="HardcodedText,ObsoleteLayoutParam"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/telefono_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_txt"
        android:layout_alignTop="@+id/list_txt"
        android:layout_marginEnd="43dp"
        android:layout_toStartOf="@+id/list_btn"
        android:layout_weight="0.03"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,RelativeOverlap" />

    <Button
        android:id="@+id/list_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        android:text="Button"
        tools:ignore="HardcodedText"
        android:layout_alignBaseline="@+id/list_txt"
        android:layout_alignBottom="@+id/list_txt"
        android:layout_alignParentEnd="true" />
</RelativeLayout>